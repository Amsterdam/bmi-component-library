{
	"name": "@amsterdam/bmi-component-library",
	"version": "1.1.6",
	"description": "A React component library based on ASC and Material-UI aimed at repurposing and sharing components across BMI projects",
	"main": "lib/index.js",
	"module": "es/index.js",
	"scripts": {
		"prepare": "npm run build:ts:es && npm run build:ts && husky install",
		"start": "npm run storybook",
		"build": "npm-run-all --parallel build:ts build:ts:es",
		"build:ts": "tsc -b src/tsconfig.build.json",
		"build:ts:watch": "tsc -b src/tsconfig.build.json --watch",
		"build:ts:es": "tsc -b src/tsconfig.build.es.json",
		"build:ts:es:watch": "tsc -b src/tsconfig.build.es.json --watch",
		"build:storybook": "build-storybook",
		"test": "jest --silent",
		"test:watch": "jest --watch",
		"lint": "npm-run-all --parallel lint:js lint:css lint:types",
		"lint:js": "eslint --ignore-path .eslintignore -c .eslintrc.js \"src/**/*.{ts,tsx,js,jsx}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
		"lint:css": "stylelint --config-basedir ./ --ignore-path .stylelintignore --config .stylelintrc.js \"src/**/*.{tsx,ts}\"",
		"lint:types": "tsc --noEmit",
		"fix": "npm-run-all --parallel fix:js fix:css",
		"fix:js": "eslint --fix --ignore-path .eslintignore -c .eslintrc.js \"src/**/*.{ts,tsx,js,jsx}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
		"ci:test": "jest --ci --silent --reporters=default",
		"ci:stylelint": "npm run lint:css",
		"ci:eslint": "npm run lint:js",
		"storybook": "start-storybook -p 6006",
		"commit": "cz",
		"release": "standard-version"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:Amsterdam/bmi-component-library.git"
	},
	"keywords": [
		"react",
		"component",
		"bmi",
		"asc",
		"material-ui"
	],
	"author": "",
	"license": "MPL-2.0",
	"devDependencies": {
		"@babel/cli": "^7.13.14",
		"@babel/core": "^7.13.14",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-decorators": "^7.13.5",
		"@babel/plugin-proposal-export-namespace-from": "^7.12.13",
		"@babel/plugin-proposal-function-sent": "^7.12.13",
		"@babel/plugin-proposal-json-strings": "^7.13.8",
		"@babel/plugin-proposal-numeric-separator": "^7.12.13",
		"@babel/plugin-proposal-throw-expressions": "^7.12.13",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-syntax-import-meta": "^7.10.4",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/runtime": "^7.14.0",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-docs": "^6.5.13",
		"@storybook/addon-essentials": "^6.5.13",
		"@storybook/addon-links": "^6.5.13",
		"@storybook/react": "^6.5.13",
		"@stylelint/postcss-css-in-js": "^0.38.0",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.5",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.2.0",
		"@types/material-ui": "^0.21.12",
		"@types/react-dom": "^17.0.17",
		"@types/react-select": "^5.0.1",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"babel-jest": "^29.2.0",
		"babel-loader": "^8.2.2",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-react-remove-properties": "^0.3.0",
		"babel-plugin-styled-components": "^2.0.7",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-webpack-alias": "^2.1.2",
		"classnames": "^2.3.2",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.2.0",
		"eslint": "^8.26.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-storybook": "^0.6.6",
		"husky": "^6.0.0",
		"jest": "^29.2.2",
		"jest-environment-jsdom": "^29.2.2",
		"jest-environment-jsdom-sixteen": "^2.0.0",
		"jest-fetch-mock": "^3.0.3",
		"jest-junit": "^14.0.1",
		"jest-styled-components": "^7.1.1",
		"lint-staged": "^13.0.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"react-select-event": "^5.5.1",
		"rxjs": "^7.5.7",
		"standard-version": "^9.5.0",
		"storybook-addon-mock": "^3.2.0",
		"stylelint": "^14.14.0",
		"stylelint-config-recommended": "^9.0.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-junit-formatter": "^0.2.2",
		"ts-jest": "^29.0.3",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@amsterdam/asc-assets": "^0.38.0",
		"@amsterdam/asc-ui": "^0.38.0",
		"@mui/icons-material": "^5.10.9",
		"@mui/material": "^5.10.11",
		"@mui/styled-engine-sc": "^5.10.6",
		"@mui/x-data-grid": "^5.17.8",
		"react": ">=17.0.2",
		"react-dom": ">=17.0.2",
		"react-dropzone": "^14.2.3",
		"react-loading-skeleton": "^3.0.3",
		"react-select": "5.7.0",
		"styled-components": ">=5.3.6"
	},
	"peerDependencies": {
		"react": ">=17.0.2",
		"react-dom": ">=17.0.2",
		"styled-components": ">=5.3.6"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write --parser \"typescript\" --loglevel error",
			"git add"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
